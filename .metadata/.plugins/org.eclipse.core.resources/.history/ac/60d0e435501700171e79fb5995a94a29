package com.selenium.Webdriver.dataproviderTests;

import java.util.HashMap;
import java.util.Iterator;
import java.util.Map;
import java.util.Map.Entry;
import java.util.Set;

import org.testng.annotations.DataProvider;

public class DataProvideClass {

	@DataProvider(name = "auth")
	public static Object[][] credentials(){
		return new Object[][]{{"name1","pwd1"},{"name2","pwd2"},{"name3","pwd3"},
				{"name4","pwd4"},{"name5","pwd5"},{"name6","pwd6"}};
	}
	
	
	
	@DataProvider(name="mapAuth")
	public static Iterator<Entry<Integer, String>> cred(){
		Map<Integer, String> map = new HashMap<Integer, String>();		
		map.put(1, "tim");map.put(2, "sam");map.put(3, "paul");map.put(4, "dan");map.put(5, "jim");
		
		Set<Entry<Integer, String>> entries = map.entrySet();
		Iterator<Entry<Integer, String>> itr = entries.iterator();
		
		return itr;
		
		
	}
	
}
