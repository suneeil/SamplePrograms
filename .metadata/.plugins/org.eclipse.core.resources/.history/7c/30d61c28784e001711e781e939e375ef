package com.org.readingData;

public class ReadTestData {
	 
     static String sheetName  = "sheet";
	  static String testcaseName = "TestA";
	  
	  
	  public static void main(String rags[]){
		  Xls_Reader xls = new Xls_Reader("C:/MyWorkSpace/DesignePattern/readingData/Data.xlsx");
		  ReadTestData r = new ReadTestData();
		  r.testStartRowNum(xls, sheetName);
	  }
	  
	  
	  public int testStartRowNum(Xls_Reader xls, String sheetName){
		  int testStartRowNum = 1;
		 	
				while(!xls.getCellData(sheetName, 0, testStartRowNum).equals(testcaseName)){
					testStartRowNum ++;
				}
				
				return testStartRowNum;
	  }
	  
}
