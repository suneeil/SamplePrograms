package com.org.readingData;

/**
 * Hello world!
 *
 */
public class ReadingData 
{
    public static void main( String[] args )
    {
       Xls_Reader xls = new Xls_Reader("C:/MyWorkSpace/DesignePattern/readingData/Data.xlsx");
       
      String sheetName  = "sheet";
		String testcaseName = "TestB";
		int testStartRowNum = 1;
     /* int col = xls.getColumnCount("sheet");
      System.out.println("Total rows: " + rows);
      System.out.println("Total columns: " + col);
      System.out.println(xls.getCellData("sheet", "username", 3));
      System.out.println(xls.setCellData("sheet", "username", 3, "SUNIL"));
      System.out.println(xls.getCellData("sheet", "username", 3));
      */
      
      
		
		while(!xls.getCellData(sheetName, 0, testStartRowNum).equals(testcaseName)){
			testStartRowNum ++;
		}
		
		System.out.println("Start Row: "+ testStartRowNum);
		int colStartRowNum = testStartRowNum + 1;
		int dataStartRowNum = testStartRowNum + 2;
		
		int rows = 0;
		System.out.println("start");
		while(xls.getCellData(sheetName, 0, dataStartRowNum+rows).equals("")){
			rows++;
		}
		
		System.out.println("Total Rows for "+ testcaseName );
    }
}
